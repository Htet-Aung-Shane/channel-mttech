<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="movies_list" name="Movies">
		<t t-call="website.layout">
			<div class="container">
				<div class="row justify-content-between">
					<t t-call="website.website_search_box_input">
						<t t-set="_form_classes" t-valuef="w-100" />
						<t t-set="_classes" t-valuef="o_lms_course_nav_search ms-1 position-relative" />
						<t t-set="_input_classes" t-valuef="border-0 rounded-0 lms_search_input bg-transparent text-white" />
						<t t-set="_submit_classes" t-valuef="btn-link text-white rounded-0 pr-1" />
						<t t-set="search_type" t-valuef="lms_courses" />
						<t t-set="action" t-valuef="/courses" />
						<t t-set="display_description" t-valuef="true" />
						<t t-set="display_detail" t-va<div class="row justify-content-between">luef="false" />
						<t t-set="placeholder">
							Search Courses
						</t>
						<t t-set="search" t-value="courses" />
					</t>
					<div class="row">
						<div class="col">
							<h1>
								Hello
							</h1>
						</div>
					</div>
				</div>
			</div>
		</t>
	</template>
</odoo>
